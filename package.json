{
  "name": "miniprogram-ts-quickstart",
  "version": "0.0.1",
  "description": "Miniapp-TypeScript-Starter-Template",
  "scripts": {
    "clean": "del-cli ./miniprogram/**/*.{js,wxss}",
    "format": "prettier --write \"miniprogram/**/*.{ts,json,scss,wxml}\"",
    "compile": "gulp",
    "dev": "gulp dev",
    "precompile": "npm run clean",
    "predev": "npm run clean"
  },
  "keywords": [],
  "author": "Tmk <i@tmk.im>",
  "contributors": [],
  "license": "UNLICENSE",
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --extends @commitlint/config-angular -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "miniprogram/**/*.{ts,json,scss,wxml}": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^9.1.2",
    "del-cli": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "miniprogram-api-typings": "^3.0.2",
    "miniprogram-ci": "^1.0.76",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  }
}
