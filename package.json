{
  "name": "miniprogram-ts-quickstart",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "del-cli ./miniprogram/**/*.{js,wxss}",
    "format": "prettier --write \"miniprogram/**/*.{ts,json,scss}\" && prettier --parser html --write \"miniprogram/**/*.wxml\"",
    "compile": "gulp",
    "dev": "gulp dev",
    "precompile": "npm run clean",
    "predev": "npm run clean"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSE",
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --extends @commitlint/config-angular -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "miniprogram/**/*.{ts,json,scss}": [
      "prettier --write"
    ],
    "miniprogram/**/*.wxml": [
      "prettier --parser html --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "del-cli": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "miniprogram-api-typings": "^2.10.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
